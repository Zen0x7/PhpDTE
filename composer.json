{
  "name": "zen0x7/phpdte",
  "version": "0.0.0",
  "description": "Librería PHP para Documentos Tributarios Electrónicos",
  "keywords": [
    "sii",
    "php",
    "dte"
  ],
  "authors": [
    {
      "name": "Ian Torres",
      "email": "iantorres@outlook.com"
    }
  ],
  "require": {
    "php": "^8.3",
    "ext-dom": "*"
  },
  "autoload": {
    "psr-4": {
      "DTE\\": "src/"
    }
  },
  "require-dev": {
    "pestphp/pest": "^2.35",
    "laravel/pint": "^1.17",
    "illuminate/support": "^11.21"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
